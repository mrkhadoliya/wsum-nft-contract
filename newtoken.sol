contract NBAPunks is ERC721,Ownable{
   using SafeMath for uint256;
    IBEP20 NBAPunk;
   
    address payable public adminAddress;
    address payable public developmentAddress;
    bool public hasSaleStarted = false;
    uint256 public _royalties;
    uint256 public developmentFees;
    uint256 public sellingFess;
    constructor() payable ERC721('NBAPunks', 'NBAPunks') 
    {
       adminAddress = payable(0x325631177F4A2DaADDf493Ab7060545914587A2d);
       developmentAddress = payable(0xC8D355019bedb8F713FF4d7D5eaCaB720ecc4BAB);
       sellingFess = 5;
       _royalties=5;
       developmentFees=5;
       
    }
    
    // The trick to change the metadata if necessary and have a reveal moment
    function setBaseURI(string memory baseURI) public onlyOwner {
        setBaseURI(baseURI);
    }

    // Start Sale, we can start minting!
    function startSale() public onlyOwner {
        hasSaleStarted = true;
    }

    // Pause Sale
    function pauseSale() public onlyOwner {
        hasSaleStarted = false;
    }
    
    function setDevelopmentFees(uint256 _developmentFees) public onlyOwner
    {
        developmentFees=_developmentFees;
    }
    
    function setDevelopmentAddress(address payable _developmentAddress) public onlyOwner
    {
        developmentAddress=_developmentAddress;
    }
    
    function setSellingFees(uint256 _sellingFess) public onlyOwner
    {
        sellingFess=_sellingFess;
    }
   
   /* createFixNFT is a function to mint fixed price NFts 
    * @_tokenURI Is the tokenUri for the Mint NFT 
    * @amount is amount to trnasfer 
    * @startId id from mint the NFT
    * @endId id to mint the NFT
    * @state is for check the BNB or token 
    * @paymentCurrency is the currency for the paymment 
    */
   
    function createFixNFT(string memory _tokenURI, uint256 amount, uint256 startId,uint256 endId ,uint256 state,address _paymentCurrency) public payable {   
    
    if ( state == 0) {
            require(msg.value >= calculatePriceBNB(endId.sub(startId)), "BNB value sent is below the price");
            adminAddress.transfer((msg.value.mul(50)).div(100));
            developmentAddress.transfer((msg.value.mul(50)).div(100));
          }
          
      else{
            NBAPunk = IBEP20(_paymentCurrency);
            require(amount >= calculatePriceToken(endId.sub(startId)), "Token sent is below the price");
            require(NBAPunk.allowance(msg.sender,address(this)) > amount, "Please allow fund first");
            NBAPunk.transferFrom(msg.sender,adminAddress,(amount.mul(50)).div(100));
            NBAPunk.transferFrom(msg.sender,developmentAddress,(amount.mul(50)).div(100));
            }   
        
         for(uint256 i=startId;i<=endId;i++)
        {
        mint(msg.sender,i,_tokenURI);
        }
       
      }
     
    /* createAuctionNFT is a function to mint Auction NFts 
    * @_tokenURI Is the tokenUri for the Mint NFT 
    * @amount is amount to trnasfer 
    * @tokenId is tokenid to mint NFT 
    * @state is for check the BNB or token 
    * @paymentCurrency is the currency for the paymment 
    */
   
    function createAuctionNFT(string memory _tokenURI,uint256 amount,uint256 tokenId,address _paymentCurrency,uint256 state) public payable
    {
       
       if ( state == 0) {
            require(msg.value >= calculatePriceBNB(tokenId), "BNB value sent is below the price");
            adminAddress.transfer((msg.value.mul(50)).div(100));
            developmentAddress.transfer((msg.value.mul(50)).div(100));
          }
          
      else{
            NBAPunk = IBEP20(_paymentCurrency);
            require(amount >= calculatePriceToken(tokenId), "Token sent is below the price");
            require(NBAPunk.allowance(msg.sender,address(this)) > amount, "Please allow fund first");
            NBAPunk.transferFrom(msg.sender,adminAddress,(amount.mul(50)).div(100));
            NBAPunk.transferFrom(msg.sender,developmentAddress,(amount.mul(50)).div(100));
            }   
          
        mint(msg.sender,tokenId,_tokenURI);
   
       
    }
   /* buyMultipleThroughBNB is a function to buy multiple nfts through BNB currency 
    * @amount is amount to trnasfer 
    * @_from id from transfer the NFT
    * @_to id to trnasfer the NFT
    * @artist is creator address 
    */
   
    function buyMultipleThroughBNB (uint256 _from,uint256 _to,address payable artist,uint256 amount) public payable
    {
       require (msg.value>0,"Must Provide Amount");
       require (msg.value>=amount,"Amount Must be Greater than");
      
       calculateBNB( _from,artist);
       for(uint256 i=_from;i<=_to;i++)
       {
        address payable  _owner   =payable(ownerOf(i));
        _transfer(_owner,msg.sender, i);
        }
        
    }
    /* buyMultipleThroughCurrencies is a function to buy multiple nfts through Other currency 
    * @buyAmount is amount to trnasfer 
    * @_from id from transfer the NFT
    * @_to id to trnasfer the NFT
    * @_artist is creator address 
    * @paymentCurrency is the currency for the paymment
    */
   
function buyMultipleThroughCurrencies (uint256 _from,uint256 _to,uint256 buyAmount,address paymentCurrency,address payable _artist)public
    {
        require(buyAmount>0,"Amount should be grater then 0 ");
        calculateToken(_from, paymentCurrency,buyAmount,_artist);
        for(uint256 i=_from;i<=_to;i++)
            {
                address payable  _owner   =payable(ownerOf(i));
                _transfer(_owner,msg.sender, i);
            }  
            
    }
      /* calculateToken is a function to calclulate the number of tokens and trnasfer in differnent wallet while trnasfer NFTs  
    * @buyAmount is amount to trnasfer 
    * @tokenId id to find out the owner of the NFT
    * @_artist is creator address 
    * @paymentCurrency is the currency for the paymment
    */
                 
function calculateToken(uint256 tokenId,address paymentCurrency, uint256 buyAmount,address payable _artist) internal 
    {
        
        uint256 owner_fees= uint256(100)-sellingFess-_royalties-developmentFees;
        NBAPunk = IBEP20(paymentCurrency);
        address  _owner = ownerOf(tokenId);
       
         if(sellingFess>0){
             require(NBAPunk.allowance(msg.sender,address(this)) > buyAmount, "Please allow fund first");
             NBAPunk.transferFrom(msg.sender,adminAddress,(buyAmount*sellingFess)/100);
         }
         if (_royalties>0)
         {
              require(NBAPunk.allowance(msg.sender,address(this)) > buyAmount, "Please allow fund first");
         NBAPunk.transferFrom(msg.sender,_artist,(buyAmount*_royalties)/100);  
         }
        if(owner_fees>0){
             require(NBAPunk.allowance(msg.sender,address(this)) > buyAmount, "Please allow fund first");
           NBAPunk.transferFrom(msg.sender,_owner,(buyAmount*owner_fees)/100); 
        }
        if(developmentFees>0)
        {
        require(NBAPunk.allowance(msg.sender,address(this)) > buyAmount, "Please allow fund first");
        NBAPunk.transferFrom(msg.sender,developmentAddress,(buyAmount*developmentFees)/100);
        }
      
    }
     /* calculateBNB is a function to calclulate BNB and trnasfer in differnent wallet while trnasfer NFTs  
    * @tokenId id to find out the owner of the NFT
    * @_artist is creator address
    */
       
    function calculateBNB(uint256 tokenId, address payable artist) public payable 
    {
        uint256 owner_fees= uint256(100)-sellingFess-_royalties-developmentFees;
        address payable  _owner =payable(ownerOf(tokenId));
        _owner.transfer((msg.value * owner_fees)/(100));
        adminAddress.transfer(( msg.value* sellingFess)/(100));
        artist.transfer((msg.value *  _royalties)/(100)); 
        developmentAddress.transfer((msg.value * developmentFees)/(100)); 
    }
    
    function calculatePriceBNB(uint currentSupply) public view returns (uint256)  {
        
        require(hasSaleStarted == true, "Sale hasn't started");
        
        if(currentSupply>=0 && currentSupply<=1999){
           return uint256(100000000000000000);
           // 0-1999 mint
          }
        if(currentSupply>=2000 && currentSupply<=3999){
           return 200000000000000000;
           // 2000-3999 mint
           }
        if(currentSupply>4000 && currentSupply<=5999){
           return 300000000000000000;
           //4000-5999 mint
        }
        if(currentSupply>6000 && currentSupply<=7999){
           return 400000000000000000;
           //6000-7999 mint
        }
        if(currentSupply>8000 && currentSupply<=8999){
           return 500000000000000000;
           //8000-8999 mint
        }
        if(currentSupply>9000 && currentSupply<=9999){
           return 1000000000000000000;
           //9000-9999 mint
        }
       
    }
    
    function calculatePriceToken(uint256 currentSupply) public view returns (uint256) {
        
        require(hasSaleStarted == true, "Sale hasn't started");
       
       if(currentSupply>=0 && currentSupply<=1999){
           return 100000000000000000000;
           // 0-1999 mint
          }
        if(currentSupply>=2000 && currentSupply<=3999){
           return 200000000000000000000;
           // 2000-3999 mint
           }
        if(currentSupply>4000 && currentSupply<=5999){
           return 300000000000000000000;
           //4000-5999 mint
        }
        if(currentSupply>6000 && currentSupply<=7999){
           return 400000000000000000000;
           //6000-7999 mint
        }
        if(currentSupply>8000 && currentSupply<=8999){
           return 500000000000000000000;
           //8000-8999 mint
        }
        if(currentSupply>9000 && currentSupply<=9999){
           return 1000000000000000000000;
           //9000-9999 mint
        }
       
    }
   
}